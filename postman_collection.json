{
  "info": {
    "name": "File Parser API",
    "description": "Postman collection for testing File Parser CRUD API with progress tracking.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Upload File",
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "file",
              "type": "file",
              "src": "example.csv"
            }
          ]
        },
        "url": {
          "raw": "http://localhost:8000/api/v1/files/",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8000",
          "path": [
            "api",
            "v1",
            "files"
          ]
        }
      }
    },
    {
      "name": "Check Progress",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "http://localhost:8000/api/v1/files/{{file_id}}/progress",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8000",
          "path": [
            "api",
            "v1",
            "files",
            "{{file_id}}",
            "progress"
          ]
        }
      }
    },
    {
      "name": "Get Parsed Content",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "http://localhost:8000/api/v1/files/{{file_id}}",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8000",
          "path": [
            "api",
            "v1",
            "files",
            "{{file_id}}"
          ]
        }
      }
    },
    {
      "name": "List Files",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "http://localhost:8000/api/v1/files/",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8000",
          "path": [
            "api",
            "v1",
            "files"
          ]
        }
      }
    },
    {
      "name": "Delete File",
      "request": {
        "method": "DELETE",
        "header": [],
        "url": {
          "raw": "http://localhost:8000/api/v1/files/{{file_id}}",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8000",
          "path": [
            "api",
            "v1",
            "files",
            "{{file_id}}"
          ]
        }
      }
    }
  ]
}